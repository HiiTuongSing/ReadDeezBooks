<h2>Books</h2>

<% books.forEach( book => { %>
 <a href="/books/<%= book.id %>">
   <img width='100' height='150' src="<%= book.coverImagePath %>" />
   <p><%=  book.title %></p>
 </a>
 <%- include("../partials/deleteForm", { url: `/books/${book.id}`})%>
 <a href="/books/<%= book.id %>/edit">Edit</a>
<% }) %>